    "gulp": "^4.0.2",
    "@babel/core": "^7.9.6",
    "gulp-imagemin": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/runtime-corejs2": "^7.9.6",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.5"
  },
  "dependencies": {
    "parcel-plugin-static-files-copy": "^2.3.1"
  }
}
